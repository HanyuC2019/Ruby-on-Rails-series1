<h1>問い合わせ一覧</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Content</th>
  </tr>

<% @contacts.each do |contact| %>
  <tr>
    <td><%= contact.name %></td>
    <td><%= contact.email %></td>
    <td><%= contact.content %></td>

    <td><%= link_to '確認', contact_path(contact.id) %></td>
    <td><%= link_to "編集", edit_contact_path(contact.id) %></td>
  </tr>
<% end %>
</table>

<br>

<%= link_to '新しく問い合せする', new_contact_path %>